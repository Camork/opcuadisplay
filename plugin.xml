<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <category
            name="OPC UA"
            icon="src/main/resources/icons/opcua.png"
            id="me.steffenjacobs.opcuadisplay">
      </category>
      <view
            name="OPC UA Explorer View"
            icon="src/main/resources/icons/view/folder.png"
            category="me.steffenjacobs.opcuadisplay"
            class="me.steffenjacobs.opcuadisplay.views.explorer.OpcUaExplorerView"
            id="me.steffenjacobs.opcuadisplay.views.explorer.OpcUaExplorerView">
      </view>
       <view
            name="OPC UA Attribute View"
            icon="src/main/resources/icons/attributes.png"
            category="me.steffenjacobs.opcuadisplay"
            class="me.steffenjacobs.opcuadisplay.views.attribute.AttributeEditorView"
            id="me.steffenjacobs.opcuadisplay.views.attribute.AttributeEditorView">
      </view>
       <view
            name="OPC UA References View"
            icon="src/main/resources/icons/view/reference.png"
            category="me.steffenjacobs.opcuadisplay"
            class="me.steffenjacobs.opcuadisplay.views.reference.ReferenceView"
            id="me.steffenjacobs.opcuadisplay.views.reference.ReferenceView">
      </view>
       <view
            name="OPC UA Star Schema View"
            icon="src/main/resources/icons/view/starschema.png"
            category="me.steffenjacobs.opcuadisplay"
            class="me.steffenjacobs.opcuadisplay.views.starschema.StarSchemaView"
            id="me.steffenjacobs.opcuadisplay.views.starschema.StarSchemaView">
      </view>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>
   
   <extension point="org.eclipse.ui.importWizards">
      <category
            name="OPC UA"
            id="me.steffenjacobs.opcuadisplay">
      </category>
      
      <wizard
            name="Import OPC UA Model"
            class="me.steffenjacobs.opcuadisplay.wizard.imp.OpcUaImportWizard"
            category="me.steffenjacobs.opcuadisplay"
            icon="src/main/resources/icons/opcua.png"
            id="me.steffenjacobs.opcuadisplay.wizard.imp.OpcUaImportWizard">
            
      </wizard>      
   </extension>
   
   <extension point="org.eclipse.ui.exportWizards">
      <category
            name="OPC UA"
            id="me.steffenjacobs.opcuadisplay">
      </category>
      
      <wizard
            name="Export OPC UA Model"
            class="me.steffenjacobs.opcuadisplay.wizard.exp.OpcUaExportWizard"
            category="me.steffenjacobs.opcuadisplay"
            icon="src/main/resources/icons/opcua.png"
            id="me.steffenjacobs.opcuadisplay.wizard.exp.OpcUaExportWizard">
            
      </wizard>      
   </extension>
   
   <extension point="org.eclipse.ui.newWizards">
      <category
            name="OPC UA"
            id="me.steffenjacobs.opcuadisplay">
      </category>
      
      <wizard
            name="OPC UA Model"
            class="me.steffenjacobs.opcuadisplay.wizard.newProject.NewProjectWizard"
            category="me.steffenjacobs.opcuadisplay"
            icon="src/main/resources/icons/opcua.png"
            id="me.steffenjacobs.opcuadisplay.wizard.newProject.NewProjectWizard">
            
      </wizard>      
   </extension>
   
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="me.steffenjacobs.opcuadisplay.perspectives.OpcUaPerspective"
            icon="src/main/resources/icons/opcua.png"
            id="me.steffenjacobs.opcuadisplay.perspectives.OpcUaPerspective"
            name="OPC UA Perspective">
      </perspective>
   </extension>

</plugin>
